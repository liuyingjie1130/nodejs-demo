{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
          "es6": true

    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "indent": [
            "error",
            4,1,2,3,5,6,7,8
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi-style": [
            "error",
            "last"
        ],
        "no-console":[
          "off"
          ]
    }
}
